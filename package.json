{
  "name": "schulconnex",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus gen-api-docs all && docusaurus build"
    "prebuild": "echo 'build:api-docs' && echo 'building...' && echo 'done'",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "build-openapi-docs": "npx docusaurus gen-api-docs all",
    "clean-openapi-docs": "git clean -dxf -- docs/generated/",
    "write-dereferenced-openapi-for-qs": "redocly bundle --dereferenced --remove-unused-components static/openapi/api-qs.yaml -o openapi-out/api-qs-deref.yaml",
    "write-dereferenced-openapi-for-dienste": "redocly bundle --dereferenced --remove-unused-components static/openapi/api-dienste.yaml -o openapi-out/api-dienste-deref.yaml",
    "validate-openapi": "npx @redocly/cli lint static/openapi/api-dienste.yaml static/openapi/api-qs.yaml"
  },
  "dependencies": {
    "@docusaurus/core": "^3.4.0",
    "@docusaurus/preset-classic": "^3.4.0",
    "@docusaurus/theme-mermaid": "^3.4.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-plugin-openapi-docs": "^3.0.1",
    "docusaurus-theme-openapi-docs": "^3.0.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.4.0",
    "@docusaurus/tsconfig": "^3.4.0",
    "@docusaurus/types": "^3.4.0",
    "@redocly/cli": "^1.19.0",
    "prettier": "^3.2.5",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  }
}
